{
  "transit": {
    "dynamicSearchWindow": {
      "minWindow": "8h",
      "maxWindow": "10h"
    }
  },
  "routingDefaults": {
    "transferSlack": 0,
    "accessEgress": {
      "maxStopCount": 100
    },
    "car": { "accelerationSpeed": 2.0 },
    "wheelchairAccessibility": {
      "trip": {
        "onlyConsiderAccessible": false,
        "unknownCost": 600,
        "inaccessibleCost": 3600
      },
      "stop": {
        "onlyConsiderAccessible": false,
        "unknownCost": 600,
        "inaccessibleCost": 3600
      }
    },
    "walk": { "reluctance": 20 },
    "itineraryFilters": {
      "accessibilityScore": true,
      "parkAndRideDurationRatio": 0.9,
      "bikeRentalDistanceRatio": 0.9,
      "nonTransitGeneralizedCostLimit": "0 + 1.5 x"
    }
  },
  "vectorTiles": {
    "layers": [
      {
        "name": "stops",
        "type": "Stop",
        "mapper": "Digitransit",
        "maxZoom": 20,
        "minZoom": 14,
        "cacheMaxSeconds": 605
      },
      {
        "name": "stations",
        "type": "Station",
        "mapper": "Digitransit",
        "maxZoom": 20,
        "minZoom": 2,
        "cacheMaxSeconds": 600
      },
      {
        "name": "rentalVehicles",
        "type": "VehicleRentalVehicle",
        "mapper": "DigitransitRealtime",
        "maxZoom": 20,
        "minZoom": 2,
        "cacheMaxSeconds": 60,
        "expansionFactor": 0.25
      },
      {
        "name": "rentalStations",
        "type": "VehicleRentalStation",
        "mapper": "DigitransitRealtime",
        "maxZoom": 20,
        "minZoom": 2,
        "cacheMaxSeconds": 60
      }
    ]
  },
  "updaters": [
    {
      "type": "real-time-alerts",
      "frequency": "20s",
      "url": "https://st-service-alerts-prod.s3.amazonaws.com/alerts.pb",
      "feedId": "40"
    }
  ]
}
